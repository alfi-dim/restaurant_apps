{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-param-reassign": "off",
        "no-useless-return": "off",
        "import/no-extraneous-dependencies": "off",
        "no-restricted-globals": "off",
        "no-console": "off",
        "no-underscore-dangle": "off"
    }
}
